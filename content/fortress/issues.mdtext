      2. group protocol attribute name:<br/>
      &lt;replace file="${dst.bootstrap.conf}
      <p>" token="@GROUP_PROTOCOL@" value="$</p>
      {group.protocol}
      <p>"/&gt;<br/>
      3. grop props attribute name: <br/>
      &lt;replace file="$</p>
      {dst.bootstrap.conf}
      <p>" token="@GROUP_PROPERTIES@" value="$</p>
      {group.properties}
      <p>"/&gt;</p>
      <br/>
    </td>
